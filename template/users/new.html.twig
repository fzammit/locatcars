{% extends 'base.html.twig' %}
{% block title %}
Users
{% endblock %}
{% block content %}
<form action="{{env.base_path  ~ '/users/'}}" method="post">

    <div class="form-group">
        <label class="FormBrand" for="firstname">Firstname</label>
        <input name="Brand" type="text">
    </div>
    <div class="form-group">
        <label class="FormModel" for="Lastname">Lastname</label>
        <input name="Model" type="text">
    </div>

    <button class="btn btn-success" type="submit">Send</button>
</form>
<div class="btn btn-primary rounded">
    <a href="{{ env.base_path ~ '/users/' }}" class="text-white">Home</a>
</div>

{% endblock %}